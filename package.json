    "leaflet": "^1.0.1",
    "moment": "^2.16.0",
    "npm": "^3.10.9",
    "query-string": "^3.0.0",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-google-maps": "^6.0.1",
    "react-leaflet": "^1.0.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.1",
    "react-search-input": "^0.10.3",
    "react-waypoint": "^3.1.1",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
